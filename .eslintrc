{
  "env": {
    "node": true
  },
  "rules": {
    "no-alert": 2,
    "no-array-constructor": 2,
    "no-bitwise": 1,
    "no-caller": 2,
    "no-catch-shadow": 2,
    "no-comma-dangle": 2,
    "no-console": 1,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-delete-var": 2,
    "no-div-regex": 2,
    "no-dupe-keys": 2,
    "no-else-return": 0,
    "no-empty": 2,
    "no-empty-class": 2,
    "no-empty-label": 2,
    "no-eq-null": 0,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-func-assign": 2,
    "no-implied-eval": 2,
    "no-iterator": 2,
    "no-label-var": 2,
    "no-loop-func": 1,
    "no-mixed-requires": [1, false],
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-new": 1,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-wrappers": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-plusplus": 0,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-return-assign": 0,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 1,
    "no-shadow": 1,
    "no-spaced-func": 0,
    "no-sync": 1,
    "no-ternary": 0,
    "no-undef": 1,
    "no-undef-init": 2,
    "no-underscore-dangle": 1,
    "no-unreachable": 2,
    "no-unused-expressions": 1,
    "no-unused-vars": 1,
    "no-use-before-define": 1,
    "no-with": 2,
    "no-wrap-func": 2,

    "block-scoped-var": 1,
    "brace-style": 1,
    "camelcase": 1,
    "complexity": [1, 11],
    "consistent-return": 1,
    "consistent-this": [1, "that"],
    "comma-spacing": 1,
    "curly": 1,
    "dot-notation": 1,
    "eqeqeq": 2,
    "guard-for-in": 1,
    "max-depth": [1, 4],
    "max-len": [1, 120, 4],
    "max-params": [1, 3],
    "max-statements": [1, 11],
    "new-cap": 1,
    "new-parens": 2,
    "one-var": 0,
    "quote-props": 0,
    "quotes": [0, "single"],
    "radix": 1,
    "semi": 1,
    "strict": 1,
    "use-isnan": 2,
    "valid-jsdoc": 2,
    "wrap-iife": 1,
    "wrap-regex": 1,
    "yoda": 1
  }
}